{
  "name": "news-alive",
  "author": {
    "name": "freddie"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "dev": "vite",
    "format": "prettier --write src/",
    "lint:fix": "eslint src --ext .js,.cjs --fix",
    "preview": "vite preview",
    "make:start": "cd workflows && make install",
    "make:stop": "cd workflows && make stop",
    "make:clean": "cd workflows && make clean",
    "make:restart": "cd workflows && make restart",
    "coverage": "vitest run --coverage",
    "pretest": "node ./test/vitest.js on",
    "test": "vitest",
    "posttest": "node ./test/vitest.js off"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "moment": "^2.29.4",
    "roboto-fontface": "*",
    "sass": "^1.62.1",
    "uuid": "^9.0.0",
    "vite": "^4.3.4",
    "vue": "^3.2.47",
    "vue-flag-icon": "^2.1.0",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.1.6",
    "vuetify": "^3.2.5",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.12.0",
    "jsdom": "^22.0.0",
    "prettier": "^2.8.8",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.31.0",
    "vue-cli-plugin-vuetify": "~2.5.8"
  }
}
